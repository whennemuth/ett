{
  "author": "Warren Hennemuth",
  "license": "ISC",
  "name": "ett",
  "version": "0.1.0",
  "bin": {
    "ett": "bin/ett.js"
  },
  "scripts": {
    "compile": "tsc",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --silent",
    "cdk": "cdk",
    "deploy": "cdk deploy --all --no-rollback --require-approval never",
    "redeploy": "npm run teardown && npm run deploy",
    "pre-teardown": "ts-node bin/pre-teardown.ts",
    "main-teardown": "bash bin/teardown.sh",
    "teardown": "npm run pre-teardown && npm run main-teardown",
    "synth": "cdk synth --all 2>&1 | tee cdk.out/ett.template.yaml",
    "trimcache": "sh bin/trim-cache.sh",
    "clearcache": "rm -rf cdk.out/* && rm -rf cdk.out/.cache/*.zip",
    "publish": "sh bin/publish.sh",
    "load-bucket": "ts-node bin/load-bucket.ts"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.352.0",
    "@types/archiver": "^6.0.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.14.6",
    "@types/uuid": "^9.0.8",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.734.0",
    "@aws-sdk/client-cloudfront": "^3.511.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.777.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.468.0",
    "@aws-sdk/client-dynamodb": "^3.429.0",
    "@aws-sdk/client-eventbridge": "^3.637.0",
    "@aws-sdk/client-iam": "^3.777.0",
    "@aws-sdk/client-lambda": "^3.637.0",
    "@aws-sdk/client-s3": "^3.410.0",
    "@aws-sdk/client-sesv2": "^3.509.0",
    "@aws-sdk/client-ssm": "^3.709.0",
    "@aws-sdk/lib-dynamodb": "^3.564.0",
    "@aws-sdk/util-dynamodb": "^3.429.0",
    "@types/uuid": "^9.0.8",
    "archiver": "^7.0.1",
    "aws-cdk": "^2.122.0",
    "aws-cdk-lib": "^2.95.1",
    "aws-sdk-client-mock": "^3.0.0",
    "aws-sdk-client-mock-jest": "^3.0.0",
    "constructs": "^10.0.0",
    "humanize-duration": "^3.32.1",
    "pdf-lib": "^1.17.1",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1"
  }
}
